{
  "extends": ["airbnb-base", "prettier"],
  "plugins": ["prettier"],
  "rules": {
    "no-console": "off",
    "no-use-before-define": ["error", { "functions": false }],
    "no-alert": "off",
    "comma-dangle": ["error", {
      "arrays": "always",
      "objects": "always"
    }],
    "arrow-parens": ["error", "always"]
  },
  "env": {
    "browser": true
  }
}
